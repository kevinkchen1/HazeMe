{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es6",
    "module": "esnext",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "noEmit": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "build"],
  "include": ["src"]
}
